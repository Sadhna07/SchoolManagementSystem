<p id="notice"><%= notice %></p>

<h1>Listing Result Infos</h1>

<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>Exam</th>
      <th>Subject</th>
      <th>Marks obtained</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @result_infos.each do |result_info| %>
      <tr>
        <td><%= result_info.student_id %></td>
        <td><%= result_info.exam_id %></td>
        <td><%= result_info.subject_id %></td>
        <td><%= result_info.marks_obtained %></td>
        <td><%= link_to 'Show', result_info %></td>
        <td><%= link_to 'Edit', edit_result_info_path(result_info) %></td>
        <td><%= link_to 'Destroy', result_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Result info', new_result_info_path %>
